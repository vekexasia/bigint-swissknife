{
  "name": "@vekexasia/bigint-constrained",
  "version": "2.0.0",
  "description": "A library to convert bigint to and from buffers with Buffer polyfills and support for uint/int",
  "scripts": {
    "build": "rm -rf dist/; tsc -b ./tsconfig.build.cjs.json ./tsconfig.build.esm.json ./tsconfig.build.types.json",
    "lint": "npx eslint --ext .ts src test",
    "compile": "rollup --config rollup.config.mjs",
    "test": "vitest",
    "benchmark": "vitest bench",
    "coverage": "vitest run --coverage",
    "pretest": "npm run build",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run test"
  },
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js",
        "default": "./build/esm/index.js"
      },
      "default": {
        "default": "./dist/index.browser.esm.js"
      },
      "types": "./dist/types/index.d.ts"
    }
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "author": {
    "url": "https://www.andreabaccega.com/",
    "email": "vekexasia@gmail.com",
    "name": "Andrea Baccega"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6"
  }
}
