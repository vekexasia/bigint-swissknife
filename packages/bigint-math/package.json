{
  "name": "@vekexasia/bigint-math",
  "version": "1.0.1",
  "description": "Math utilities for BigInt: abs, sign, max, min, bitLength, divide, rand",
  "main": "./dist/node.cjs.js",
  "module": "./dist/node.esm.mjs",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "lint": "eslint \"**/*.ts\"",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "pretest": "npm run build",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run build && vitest run"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "import": "./dist/node.esm.mjs",
        "require": "./dist/node.cjs.js",
        "default": "./dist/node.cjs.js"
      },
      "default": {
        "import": "./dist/browser.esm.mjs",
        "default": "./dist/browser.cjs.js"
      }
    }
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vekexasia/bigint-swissknife.git",
    "directory": "packages/bigint-math"
  },
  "keywords": [
    "bigint",
    "math",
    "abs",
    "sign",
    "max",
    "min",
    "random",
    "bitlength"
  ],
  "author": {
    "url": "https://www.andreabaccega.com/",
    "email": "vekexasia@gmail.com",
    "name": "Andrea Baccega"
  },
  "license": "MIT",
  "peerDependencies": {
    "@vekexasia/bigint-uint8array": "*"
  }
}
