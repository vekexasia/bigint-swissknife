{
  "name": "@vekexasia/bigint-uint8array",
  "version": "1.1.1",
  "description": "Convert BigInt to/from Uint8Array with bounds checking, signed/unsigned support, and big/little endian",
  "main": "./dist/node.cjs.js",
  "module": "./dist/node.esm.mjs",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "lint": "npx eslint --ext .ts src test",
    "test": "vitest",
    "benchmark": "vitest bench",
    "coverage": "vitest run --coverage",
    "pretest": "npm run build",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run build && vitest run"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "import": "./dist/node.esm.mjs",
        "default": "./dist/node.cjs.js"
      },
      "default": "./dist/browser.esm.mjs"
    }
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vekexasia/bigint-swissknife.git",
    "directory": "packages/bigint-uint8array"
  },
  "keywords": [
    "bigint",
    "uint8array",
    "buffer",
    "conversion",
    "signed",
    "unsigned",
    "endian"
  ],
  "author": {
    "url": "https://www.andreabaccega.com/",
    "email": "vekexasia@gmail.com",
    "name": "Andrea Baccega"
  },
  "license": "MIT",
  "peerDependencies": {
    "@vekexasia/bigint-buffer2": "*"
  },
  "peerDependenciesMeta": {
    "@vekexasia/bigint-buffer2": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vekexasia/bigint-buffer2": "workspace:*"
  }
}
